<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <record model="ir.module.category" id="administracion_modulo_laboratorio">
      <field name="name">Modulo de Laboratorio</field>
      <field name="description">Usuarios que accesan a este modulo</field>
      <field name="sequence">3</field>
    </record>

   <record id="group_user" model="res.groups">
     <field name="name">Usuario</field>
     <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
     <field name="users" eval="[(4, ref('base.user_root'))]"/>
     <field name="category_id" ref="laboratorio_hh.grupo_administrador"/>
    </record>

    <record id="group_manager" model="res.groups">
      <field name="name">Administrador</field>
      <field name="implied_ids" eval="[(4, ref('simcard.group_user'))]"/>
      <field name="category_id" ref="laboratorio_hn.grupo_usuario"/>
      <field name="users" eval="[(4, ref('base.user_root'))]"/>
    </record>
  </data>
</odoo>